package com.ve.itemcenter.common.constant;

public enum DBConst {
	/**
	 * 品牌状态
	 */
	BRAND_NORMAL(0),

	/**
	 * 商品进入回收站的状态
	 */
	IN_TRASH(1),

	/**
	 * 已删除
	 */
	DELETED(1),
	/**
	 * 未删除
	 */
	UN_DELETED(0),

	/**
	 * 待审核
	 */
	NOT_AUDITED(0),

	/**
	 * 审核中
	 */
	AUDITING(1),

	/**
	 * 审核通过
	 */
	AUDIT_PASS(2),

	/**
	 * 审核未通过
	 */
	AUDIT_UNPASS(3);

	private int code;

	private DBConst(int code) {
		this.code = code;
	}

	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

}
