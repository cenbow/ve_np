package com.ve.marketingcenter.common.constant.coupon;

public enum CouponStatus {
	/**
	 * 未使用
	 */
	UN_USE(0),
	/**
	 * 冻结
	 */
	FREEZE(1),
	/**
	 * 已使用
	 */
	USED(2);

	private Integer value;

	private CouponStatus(Integer value) {
		this.value = value;
	}

	public Integer getValue() {
		return value;
	}
}
